{
    "workbench.colorTheme": "Celestial",
    "workbench.colorCustomizations" : {
        
        "editor.background": "#121417",

        "editorGroupHeader.tabsBorder": "#1e2025",  // for the border below the tabs.

        "tab.activeBackground": "#121417",
        "tab.activeForeground": "#bcee9fd4",
        "tab.activeBorder": "#121417",
        "tab.border": "#1e2025",
        "tab.inactiveBackground": "#121212",
        "tab.inactiveForeground": "#9E9E9E",


        "activityBarBadge.background": "#829ab1",
        "activityBar.foreground": "#829ab1",

        "editorCursor.foreground": "#bb86fcEB",

        "menu.foreground": "#bcccdcf2",
        "menu.background": "#1a1c23",

        "editor.selectionBackground": "#242630cc",
        "editor.selectionHighlightBackground": "#242630cc",
        "editor.hoverHighlightBackground": "#242630dd",
        "editor.lineHighlightBackground": "#1b1d22d2",


        
    },
    
  
    



    "editor.fontFamily": "Ellograph CF ,monospace",
    "editor.fontWeight": "300",
    "editor.letterSpacing": 0.15,
    "editor.lineHeight": 1.5, 
    "editor.fontLigatures": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": 
        [
            {
                "scope":"keyword.operator, variable.parameter, storage.modifier, storage.type.built-in.primitive.cpp",
                "settings": 
                {
                    "fontStyle": ""
                }
            },
        {
            "scope":"constant.other.caps.python",
            "settings":{
                "foreground": "#c5cef3df"
            }
        },
        
        {
            "scope" : "meta.function-call.generic.python",
            "settings":{
                "foreground": "#DDA5E8EC",
                
            }
        },
        {
            "scope" : "keyword.operator.increment.cpp, keyword.operator.decrement.cpp, storage.modifier.pointer.cpp, keyword.operator.arithmetic.cpp",
            "settings": {
                "foreground": "#85CA46",
            }
        },
        {
            "scope": "punctuation.separator.pointer-access.cpp",
            "settings": {
                "foreground": "#ffffffdc",
            }
        },
        {
            "scope" : "meta.body.class.cpp",
            "settings" : {
                "foreground": "#B0C4DE"
            }
        },
        {
            "scope" : "punctuation.section.block.begin.bracket.curly.function.definition, punctuation.section.block.end.bracket.curly.cpp, punctuation.section.block.end.bracket.curly.class.cpp",
            "settings":{
                "foreground": "#BBBBBB"
            }
        },
        {
            "scope": "support.function.builtin.python",
            "settings" :{
                "foreground": "#25B0BCE6",
            }
        },
        {
            "scope": "meta.function-call.arguments.python",
            "settings" :{
                "foreground": "#718CD2",
            }
        },
        {
            "scope": "source.python",
            "settings" :{
                "foreground": "#FAF0E6CE",
            }
        },
        {
            "scope":"punctuation.separator.period.python,keyword.operator.assignment.python",
            "settings" :{
                "foreground": "#E95678E6",
            }
        },
        {
            "scope": "punctuation.definition.arguments.begin.python,punctuation.definition.arguments.end.python",
            "settings":{
                "foreground": "#cfa592",
            }
        },
        {
            "scope": "meta.attribute.python,keyword.operator.arithmetic.python",
            "settings":{
                "foreground": "#25B0BCE6",
            }
        },
        {
            "scope": "string.interpolated.double.dart,string.interpolated.single.dart",
            "settings":{
                "foreground": "#DFA974",
        
            }
        },
        {
            "scope": "text.html",
            "settings":{
                "foreground": "#B5EECBA5",
                
            }
        },
        {
            "scope": "source.css, support.type.property-name.css, entity.other.attribute-name",
            "settings":{
                "foreground": "#CCCCFFA5",
            }
        },
        {
            "scope":"entity.other.attribute-name.html",
            "settings": {
                "foreground": "#cec2eba7",
                
            }
        },
        {
            "scope":"entity.other.attribute-name.class.css",
            "settings":{
                "foreground": "#B5EECBA5",
            }   
        }
    ],
    "types": "#cec2eb",
    },
    "editor.fontSize": 18
    ,
    "python.defaultInterpreterPath": "C:\\Users\\Skylor\\AppData\\Local\\Programs\\Python\\Python39\\python.exe",
    



    "shellLauncher.shells.windows": [
        {
            "shell":"C:\\cygwin64\\bin\\bash.exe",
            "args" :["--login"],
            "label":"Cygwin Bash",
            "launchName":"Cygwin Bash"
        },
        {
            "shell": "C:\\Windows\\System32\\cmd.exe",
            "label": "cmd"
        },
        {
            "shell": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe",
            "label": "PowerShell"
        },
        {
            "shell": "C:\\Program Files\\Git\\bin\\bash.exe",
            "label": "Git bash"
        },
        {
            "shell": "C:\\Windows\\System32\\bash.exe",
            "label": "WSL Bash"
        }
    ],
    "kite.showWelcomeNotificationOnStartup": false,
    "tabnine.experimentalAutoImports": true,
    "explorer.confirmDelete": false,
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "security.workspace.trust.untrustedFiles": "open",
    "markdown.preview.fontFamily": "JetBrains Mono",
    "markdown.preview.fontSize": 13,
    "notebook.markup.fontSize": 16,
    "files.associations": {
        "*.py": "python"
    },
    
    "git.autofetch": true,
    "editor.largeFileOptimizations": false,
    "jupyter.askForKernelRestart": false,
    "window.zoomLevel": -1,
    "editor.unicodeHighlight.nonBasicASCII": false,
    "editor.inlineSuggest.enabled": true,
    "workbench.iconTheme": "city-lights-icons-vsc",
    "window.autoDetectHighContrast": false,
    "breadcrumbs.filePath": "off"
}



